<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMobile.AltaCliente2">
    <ScrollView >
        <StackLayout Padding="20">

            <Label Text="Alta Nuevo Cliente" Font="25" FontAttributes="Bold" TextColor="Navy" HorizontalOptions="Center" />

            <Label Text="RUT:" TextColor="#000000"></Label>
            <Label x:Name="lblRut" TextColor="#7B7776" />
            
            <Label></Label>
            <Label Text="Nombre Empresa:" TextColor="#000000"></Label>
            <Label x:Name="lblNombreEmp"  TextColor="#7B7776"/>
            
            <Label></Label>
            <Label Text="Direccion:" TextColor="#000000"></Label>
            <Label x:Name="lblDireccion" TextColor="#7B7776"/>
                 
            <Label></Label>
            <Label Text="Telefono:" TextColor="#000000" ></Label>
            <Label x:Name="lblTelefono"  TextColor="#7B7776"/>
           
            <Label></Label>
            <Label Text="Ciudad:" TextColor="#000000"></Label>
            <Label x:Name="lblCiudad"  TextColor="#7B7776"/>
          
            <Label></Label>
            <Button x:Name="btnConfirmar" Text="Confirmar"></Button>
            <Button x:Name ="btnCancelar" Text="Cancelar">
            </Button>

        </StackLayout>
    </ScrollView>
</ContentPage>