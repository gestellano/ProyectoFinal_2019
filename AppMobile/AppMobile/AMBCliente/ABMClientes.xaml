<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMobile.ABMClientes">
    
    <ScrollView >
 
        <StackLayout Padding="20">

            <Label Text="A-M Clientes" Font="25" FontAttributes="Bold" TextColor="Navy" HorizontalOptions="Center" />

            <Label Text="RUT:" TextColor="#000000" ></Label>
            <Entry x:Name="lblRut" Placeholder="rut" TextColor="#000000" Keyboard="Telephone"></Entry>
       
            <Label></Label>
            <Button x:Name="btnBuscar" Text="Buscar"></Button>
            <Button x:Name="btnCancelar" Text="Cancelar"></Button>
            
            


        </StackLayout>
    </ScrollView>
</ContentPage>